
            <div class="row">
                <div class="col-lg-12">
                    <h1>Dashboard</h1>
                    <div class="alert alert-dismissable alert-warning">
                        <button data-dismiss="alert" class="close" type="button">&times;</button>
                        Welcome to the admin dashboard!
                    </div>
                </div>
            </div>

            <% for ( var room in def.rooms ) {
                var volets =def.rooms [room] ;
                //console.log ('room =' + room) ;
                %>
            <div class="row">
                <div class="col-md-8">
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-bar-chart-o"></i> <%= room %></h3>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="panel panel-default">
                                        <div class="panel-body">&nbsp;</div>
                                    </div>
                                </div>
                                <button id="<%= room %>_1" type="button" class="btn btn-primary btn-lg btn-success">Ouvrir</button>
                                <button id="<%= room %>_2" type="button" class="btn btn-primary btn-lg btn-warning">a demi</button>
                                <button id="<%= room %>_0" type="button" class="btn btn-primary btn-lg btn-danger">Fermer</button>
                            </div>
                            <script>
                                <% for ( var ip =0 ; ip <= 2 ; ip++ ) { %>
                                $('#<%= room %>_<%= ip %>').click (function (evt) {
                                    evt.stopPropagation () ;
                                    $.ajax ({
                                        url: 'http://' + window.location.host + '/room/<%= room %>/<%= ip %>',
                                        type: 'get',
                                        //data: JSON.stringify ({}),
                                        //contentType: 'application/json',
                                        complete: null
                                    }).done (function (data) {
                                    }) ;
                                }) ;
                                <% } %>
                            </script>
                            <% for ( var iv =0 ; iv < volets.length ; iv++ ) {
                                var voletid =volets [iv] ;
                                //console.log ('voletid =' + voletid) ;
                                var volet =def.cmds [voletid] ;
                                //console.log ('volet =' + JSON.stringify(volet)) ;
                                %>
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="panel panel-default">
                                        <div class="panel-body"><%= volet.name %></div>
                                    </div>
                                </div>
                                <button id="<%= voletid %>_1" type="button" class="btn btn-primary btn-lg btn-success">Ouvrir</button>
                                <button id="<%= voletid %>_2" type="button" class="btn btn-primary btn-lg btn-warning">a demi</button>
                                <button id="<%= voletid %>_0" type="button" class="btn btn-primary btn-lg btn-danger">Fermer</button>
                            </div>
                            <script>
                                <% for ( var ip =0 ; ip < volet.pins.length ; ip++ ) { %>
                                $('#<%= volets [iv] %>_<%= ip %>').click (function (evt) {
                                    evt.stopPropagation () ;
                                    $.ajax ({
                                        url: 'http://' + window.location.host + '/volet/<%= voletid %>/<%= ip %>',
                                        type: 'get',
                                        //data: JSON.stringify ({}),
                                        //contentType: 'application/json',
                                        complete: null
                                    }).done (function (data) {
                                    }) ;
                                }) ;
                                <% } %>
                            </script>
                            <% } %>

                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    
                </div>
            </div>
            <% } %>
            